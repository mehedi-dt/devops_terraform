{
    "Version": "2012-10-17",
    "Statement": [
        {
        "Sid": "AllowManageBucketContents",
        "Effect": "Allow",
        "Action": [
            "s3:PutObject",
            "s3:GetObject"
        ],
        "Resource": [
            "arn:aws:s3:::${bucket_name}/*"
        ]
        }
    ]
}